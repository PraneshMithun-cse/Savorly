{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/login",
            "dest": "/api/index.js"
        },
        {
            "src": "/signin",
            "dest": "/api/index.js"
        },
        {
            "src": "/admin",
            "dest": "/api/index.js"
        },
        {
            "src": "/delivery",
            "dest": "/api/index.js"
        },
        {
            "src": "/orders",
            "dest": "/api/index.js"
        },
        {
            "src": "/cart",
            "dest": "/api/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.png|.*\\.jpg|.*\\.svg|.*\\.ico|.*\\.webp)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}